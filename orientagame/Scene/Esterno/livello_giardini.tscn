[sub_resource type="GDScript" id="GDScript_mtb7v"]
script/source = "extends CharacterBody3D

@export var speed: float = 2.0
@onready var sprite: AnimatedSprite3D = $Sprite 

func _ready():
	if has_node(\"Sprite\"):
		print(\"Nodo Sprite trovato:\", $Sprite)
	else:
		printerr(\"Nodo Sprite NON trovato: controlla nome e percorso\")

func set_sprite_frames(frames: SpriteFrames) -> void:
	if sprite:
		sprite.sprite_frames = frames
		sprite.play(\"idle\")
	else:
		push_error(\"Nodo Sprite non trovato nell'NPC_Base.tscn\")

func update_animation(direction: Vector3) -> void:
	if not sprite:
		return
	if direction.length() == 0:
		sprite.play(\"idle\")
		return
	if abs(direction.x) > abs(direction.z):
		sprite.play(\"move_right\" if direction.x > 0 else \"move_left\")
	else:
		sprite.play(\"move_down\" if direction.z > 0 else \"move_up\")
"

